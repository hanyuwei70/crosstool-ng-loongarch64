CT_BINUTILS_SRC_DEVEL=y
CT_BINUTILS_DEVEL_REVISION="a55939ca586b81c16cc95fa692a7862551cc6773"

CT_GLIBC_SRC_DEVEL=y
CT_GLIBC_DEVEL_URL="https://github.com/loongson/glibc.git"
CT_GLIBC_DEVEL_REVISION="c0ae2f4dccd67ef22ddb44c564cf3dabe6635853"
CT_GLIBC_ENABLE_WERROR=n